[
    {
        "path": "/api/v1/blocks",
        "method": "GET",
        "description": "View blocked users",
        "return": "Array Account",
        "parameters": [
            {
                "name": "max_id",
                "type": "Integer",
                "required": false
            },
            {
                "name": "since_id",
                "type": "Integer",
                "required": false
            },
            {
                "name": "min_id",
                "type": "Integer",
                "required": false
            },
            {
                "name": "limit",
                "type": "Integer",
                "required": false
            }
        ]
    },
    {
        "path": "/api/v1/accounts/:id/block",
        "method": "POST",
        "description": "Block account",
        "return": "Relationship",
        "parameters": [
            {
                "name": ":id",
                "type": "String",
                "required": true
            }
        ]
    },
    {
        "path": "/api/v1/accounts/:id/unblock",
        "method": "POST",
        "description": "Unblock account",
        "return": "Relationship",
        "parameters": [
            {
                "name": ":id",
                "type": "String",
                "required": true
            }
        ]
    }
]
